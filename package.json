{
    "name": "merestorage",
    "version": "0.0.0",
    "private": true,
    "description": "",
    "license": "UNLICENSED",
    "author": "",
    "scripts": {
        "bootstrap": "yarn ci",
        "prebuild": "rimraf dist",
        "build": "yarn run build:back && yarn run build:front",
        "build:back": "cd backend && nest build",
        "build:front": "cd client && tsc && vite build",
        "deps:update": "ncu -u",
        "prelint:es": "prettier --write \"**/*.{ts,js,tsx}\"",
        "lint:es": "eslint --fix \"**/*.{ts,js,tsx}\"",
        "prelint:es:ci": "prettier --check \"**/*.{ts,js,tsx}\"",
        "lint:es:ci": "eslint \"**/*.{ts,js,tsx}\"",
        "start:back": "cd backend && nest start --watch",
        "start:back:debug": "cd backend && nest start --debug --watch",
        "start:back:prod": "node dist/backend/src/main",
        "start:front": "cd client && vite",
        "test": "jest",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "prisma": "cd backend && yarn pnpify prisma"
    },
    "dependencies": {
        "@ant-design/icons": "5.0.1",
        "@nestjs/common": "9.3.9",
        "@nestjs/config": "2.3.1",
        "@nestjs/core": "9.3.9",
        "@nestjs/platform-express": "9.3.9",
        "@nestjs/schedule": "2.2.0",
        "@nestjs/swagger": "6.2.1",
        "@nestjs/terminus": "9.2.1",
        "@prisma/client": "4.11.0",
        "@reduxjs/toolkit": "1.9.3",
        "@willsoto/nestjs-prometheus": "5.1.0",
        "antd": "5.2.3",
        "axios": "1.3.4",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "cron": "2.2.0",
        "cronstrue": "2.23.0",
        "express": "4.18.2",
        "http-status-codes": "2.2.0",
        "nestjs-pino": "3.1.2",
        "pg": "8.9.0",
        "pino": "8.11.0",
        "pino-http": "8.3.3",
        "prom-client": "14.1.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.43.4",
        "react-redux": "8.0.5",
        "react-router-dom": "6.8.2",
        "reflect-metadata": "0.1.13",
        "rimraf": "4.3.0",
        "rxjs": "7.8.0"
    },
    "devDependencies": {
        "@faker-js/faker": "7.6.0",
        "@nestjs/cli": "9.2.0",
        "@nestjs/testing": "9.3.9",
        "@types/cron": "2.0.0",
        "@types/express": "4.17.17",
        "@types/jest": "29.4.0",
        "@types/node": "18.14.6",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11",
        "@types/react-redux": "7.1.25",
        "@typescript-eslint/eslint-plugin": "5.54.0",
        "@typescript-eslint/parser": "5.54.0",
        "@vitejs/plugin-react": "3.1.0",
        "@yarnpkg/pnpify": "4.0.0-rc.39",
        "eslint": "8.35.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-jest": "27.2.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.32.2",
        "jest": "29.4.3",
        "npm-check-updates": "16.7.10",
        "prettier": "2.8.4",
        "prisma": "4.11.0",
        "sass": "1.58.3",
        "source-map-support": "0.5.21",
        "ts-jest": "29.0.5",
        "ts-node": "10.9.1",
        "typescript": "4.9.5",
        "vite": "4.1.4"
    },
    "packageManager": "yarn@3.3.1",
    "prisma": {
        "schema": "backend/src/prisma/schema.prisma"
    },
    "resolutions": {
        "@prisma/client/.prisma": "link:node_modules/.prisma"
    }
}
