{
    "name": "merestorage",
    "version": "0.0.0",
    "private": true,
    "description": "",
    "license": "UNLICENSED",
    "author": "",
    "scripts": {
        "bootstrap": "yarn ci",
        "prebuild": "rimraf dist",
        "build": "yarn run build:back && yarn run build:front",
        "build:back": "cd backend && nest build",
        "build:front": "cd client && tsc && vite build",
        "build:front:visualize": "cd dist/client && vite-bundle-visualizer",
        "deps:update": "ncu -u",
        "prelint:es": "prettier --write \"**/*.{ts,js,tsx}\"",
        "lint:es": "eslint --fix \"**/*.{ts,js,tsx}\"",
        "prelint:es:ci": "prettier --check \"**/*.{ts,js,tsx}\"",
        "lint:es:ci": "eslint \"**/*.{ts,js,tsx}\"",
        "start:back": "cd backend && nest start --watch",
        "start:back:debug": "cd backend && nest start --debug --watch",
        "start:back:prod": "node dist/backend/src/main",
        "start:front": "cd client && vite",
        "test": "jest",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "prisma": "cd backend && yarn pnpify prisma"
    },
    "dependencies": {
        "@ant-design/icons": "5.0.1",
        "@ant-design/plots": "1.2.5",
        "@nestjs/common": "9.4.0",
        "@nestjs/config": "2.3.1",
        "@nestjs/core": "9.4.0",
        "@nestjs/platform-express": "9.4.0",
        "@nestjs/swagger": "6.3.0",
        "@nestjs/terminus": "9.2.2",
        "@prisma/client": "4.13.0",
        "@reduxjs/toolkit": "1.9.5",
        "@willsoto/nestjs-prometheus": "5.1.1",
        "antd": "5.4.5",
        "cassandra-driver": "4.6.4",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "cron-parser": "4.8.1",
        "cronstrue": "2.26.0",
        "express": "4.18.2",
        "nestjs-pino": "3.1.3",
        "pg": "8.10.0",
        "pg-boss": "9.0.0",
        "pino": "8.11.0",
        "pino-http": "8.3.3",
        "prom-client": "14.2.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.43.9",
        "react-redux": "8.0.5",
        "react-router-dom": "6.10.0",
        "reflect-metadata": "0.1.13",
        "rimraf": "5.0.0",
        "rxjs": "7.8.0"
    },
    "devDependencies": {
        "@faker-js/faker": "7.6.0",
        "@nestjs/cli": "9.4.2",
        "@nestjs/testing": "9.4.0",
        "@types/express": "4.17.17",
        "@types/jest": "29.5.1",
        "@types/node": "18.16.0",
        "@types/pg": "8.6.6",
        "@types/react": "18.0.38",
        "@types/react-dom": "18.0.11",
        "@types/react-redux": "7.1.25",
        "@typescript-eslint/eslint-plugin": "5.59.0",
        "@typescript-eslint/parser": "5.59.0",
        "@vitejs/plugin-react": "4.0.0",
        "@yarnpkg/pnpify": "4.0.0-rc.42",
        "eslint": "8.39.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-jest": "27.2.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.32.2",
        "husky": "8.0.3",
        "jest": "29.5.0",
        "lint-staged": "13.2.1",
        "npm-check-updates": "16.10.9",
        "prettier": "2.8.8",
        "prisma": "4.13.0",
        "rollup-plugin-visualizer": "5.9.0",
        "sass": "1.62.0",
        "source-map-support": "0.5.21",
        "stylelint": "15.6.0",
        "stylelint-prettier": "3.0.0",
        "stylelint-scss": "4.6.0",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.1",
        "typescript": "4.9.5",
        "vite": "4.3.1"
    },
    "packageManager": "yarn@3.3.1",
    "prisma": {
        "schema": "backend/src/prisma/schema.prisma"
    },
    "resolutions": {
        "@prisma/client/.prisma": "link:node_modules/.prisma"
    }
}
