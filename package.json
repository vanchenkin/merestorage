{
    "name": "merestorage",
    "version": "0.0.0",
    "private": true,
    "description": "",
    "license": "UNLICENSED",
    "author": "",
    "scripts": {
        "bootstrap": "yarn ci",
        "prebuild": "rimraf dist",
        "build": "yarn run build:back && yarn run build:front",
        "build:back": "cd backend && nest build",
        "build:front": "cd client && tsc && vite build",
        "deps:update": "ncu -u",
        "prelint:es": "prettier --write \"**/*.{ts,js,tsx}\"",
        "lint:es": "eslint --fix \"**/*.{ts,js,tsx}\"",
        "prelint:es:ci": "prettier --check \"**/*.{ts,js,tsx}\"",
        "lint:es:ci": "eslint \"**/*.{ts,js,tsx}\"",
        "start:back": "cd backend && nest start --watch",
        "start:back:debug": "cd backend && nest start --debug --watch",
        "start:back:prod": "node dist/backend/src/main",
        "start:front": "cd client && vite",
        "test": "jest",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "knex": "knex --knexfile backend/src/scripts/knexfile.ts --migrations-directory ../database/migrations",
        "migrate": "knex migrate:latest"
    },
    "dependencies": {
        "@nestjs/common": "9.3.2",
        "@nestjs/config": "2.3.0",
        "@nestjs/core": "9.3.2",
        "@nestjs/platform-express": "9.3.2",
        "@nestjs/platform-fastify": "9.3.2",
        "@nestjs/schedule": "2.2.0",
        "@nestjs/serve-static": "3.0.0",
        "@nestjs/swagger": "6.1.4",
        "@nestjs/terminus": "9.2.0",
        "@reduxjs/toolkit": "1.9.2",
        "@willsoto/nestjs-objection": "8.1.0",
        "@willsoto/nestjs-prometheus": "5.1.0",
        "antd": "5.1.7",
        "axios": "1.3.2",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "cron": "2.2.0",
        "cronstrue": "2.22.0",
        "express": "4.18.2",
        "http-status-codes": "2.2.0",
        "knex": "2.4.2",
        "nestjs-pino": "3.1.2",
        "objection": "3.0.1",
        "pg": "8.9.0",
        "pino": "8.8.0",
        "pino-http": "8.3.2",
        "prom-client": "14.1.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.43.0",
        "react-redux": "8.0.5",
        "react-router-dom": "6.8.0",
        "reflect-metadata": "0.1.13",
        "rimraf": "4.1.2",
        "rxjs": "7.8.0"
    },
    "devDependencies": {
        "@faker-js/faker": "7.6.0",
        "@nestjs/cli": "9.2.0",
        "@nestjs/testing": "9.3.2",
        "@types/cron": "2.0.0",
        "@types/express": "4.17.17",
        "@types/jest": "29.4.0",
        "@types/node": "18.11.19",
        "@types/react": "18.0.27",
        "@types/react-dom": "18.0.10",
        "@types/react-redux": "7.1.25",
        "@typescript-eslint/eslint-plugin": "5.50.0",
        "@typescript-eslint/parser": "5.50.0",
        "@vitejs/plugin-react": "3.1.0",
        "eslint": "8.33.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-jest": "27.2.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.32.2",
        "jest": "29.4.1",
        "npm-check-updates": "16.6.3",
        "prettier": "2.8.3",
        "sass": "1.58.0",
        "source-map-support": "0.5.21",
